
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read, write on any file in the /resources path for any authenticated user.
    match /resources/{companyId}/{userId}/{allPaths=**} {
      allow read: if request.auth != null;
      allow write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
